{
  "expo": {
    "name": "turtle-cli-test",
    "slug": "turtle-cli-test",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.smnodame.test"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smnodame.test",
      "buildNumber": "13",
      "config": {
        "usesNonExemptEncryption": false
      }
    }
  }
}
